{
  "private": true,
  "name": "react-redux-boilerplate",
  "version": "1.0.0-beta",
  "description": "A react, redux, react-router, webpack boilerplate.",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "postinstall": "cross-env NODE_ENV=development npm run build:vendor",
    "clean": "rimraf dist/",
    "dev": "webpack-dev-server --config build/webpack.dev.config.js & npm run mock",
    "mock": "nodemon --watch mock build/mock-server.js",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production npm run build:vendor && npm run build:prod",
    "build:prod": "webpack --config build/webpack.prod.config.js --progress --hide-modules --display-optimization-bailout",
    "build:vendor": "webpack --config build/webpack.vendor.config.js --progress --hide-modules --display-optimization-bailout",
    "lint": "eslint src/ --fix; exit 0"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/evan2x/react-redux-boilerplate.git"
  },
  "author": {
    "name": "evan2x",
    "email": "evan2zaw@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/evan2x/react-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/evan2x/react-redux-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.2",
    "add-asset-html-webpack-plugin": "^2.1.3",
    "chalk": "^2.4.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^3.4.2",
    "mini-css-extract-plugin": "^0.4.2",
    "nodemon": "^1.18.4",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "pre-commit": "^1.2.2",
    "react-hot-loader": "^4.3.5",
    "require-dir": "^1.0.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "axios": "^0.18.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router": "^3.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-saga": "^0.16.0"
  }
}
