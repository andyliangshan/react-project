    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.3.1",
    "nodemon": "^1.11.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-loader": "^2.0.5",
    "react-hot-loader": "^3.0.0-beta.7",
    "require-dir": "^0.3.1",
    "rimraf": "^2.6.1",
