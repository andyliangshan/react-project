    "require-dir": "^1.2.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1",
    "webpackbar": "^3.2.0"
  },
  "dependencies": {
    "@loadable/component": "^5.9.0",
    "axios": "^0.18.1",
