{
  "private": true,
  "name": "react-redux-boilerplate",
  "version": "1.0.0-alpha",
  "description": "A react, redux, react-router, webpack boilerplate.",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "dev": "rimraf dist/ && webpack-dev-server --config build/webpack.dev.config.js & npm run mock",
    "mock": "nodemon --watch mock build/mock-server.js",
    "build": "rimraf dist/ && npm run build:prod",
    "build:prod": "cross-env NODE_ENV=production webpack --config build/webpack.prod.config.js --progress --hide-modules",
    "lint": "eslint src/; exit 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/evan2x/react-redux-boilerplate.git"
  },
  "author": {
    "name": "evan2x",
    "email": "evan2zaw@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/evan2x/react-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/evan2x/react-redux-boilerplate#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chalk": "^1.1.3",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.2",
    "eslint": "^3.19.0",
