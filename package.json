    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "nodemon": "^1.12.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.6",
    "react-hot-loader": "^4.0.0-beta.16",
    "require-dir": "^0.3.2",
