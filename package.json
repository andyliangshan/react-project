  "version": "0.3.0-alpha",
  "description": "A react, redux, react-router, webpack boilerplate.",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "lint": "eslint src; exit 0",
    "dev": "rimraf dist/ && webpack-dev-server --config build/webpack.dev.config.js",
    "mock": "nodemon --watch mock build/mock-server.js",
    "build": "rimraf dist/ && npm run build:prod",
    "build:prod": "cross-env NODE_ENV=production webpack --config build/webpack.prod.config.js --progress --hide-modules"
  },
